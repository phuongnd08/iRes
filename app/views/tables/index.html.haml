#table_selector_page{ "data-role" => "page" }
  %div{ "data-role" => "header" }
    %h1= t("waiter.order_table")
    %div{ "data-role" => "navbar" }
      %ul
        - 10.times do |time|
          %li
            %a.table{ :href => new_order_path(:table_number => time + 1), "rel" => "external", "data-table-number" => time + 1 }= t("order.table_no", :no => time + 1)
